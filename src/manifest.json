{
  "name": "Incognito Search",
  "description": "Right click to search the word/phrase in incognito",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "tabs",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": { "keyword" : "nt" },
  "action": {
    "default_icon": {
      "16": "incognito_searchx16.png",
      "32": "incognito_searchx32.png"
      }
  },
  "icons": {
    "16": "incognito_searchx16.png",
    "32": "incognito_searchx32.png",
    "48": "incognito_searchx48.png",
    "128": "incognito_searchx128.png"
  }
}
